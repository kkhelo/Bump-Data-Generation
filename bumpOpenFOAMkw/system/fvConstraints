FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitT
{
    type       limitTemperature;

    min        150;
    max        1500;

    selectionMode all;
}

limitP
{
    type       limitPressure;

    minFactor  0.5;
    maxFactor  6.0;
}

