FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 13.5;

boundaryField
{
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.005;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    top
    {
        type            symmetryPlane;
    }

    left
    {
        type            symmetryPlane;
    }
    
    right
    {
        type            symmetryPlane;
    }

    bump
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    plate
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //